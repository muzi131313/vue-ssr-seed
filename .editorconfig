#doc https://www.vanadis.cn/2017/03/25/editor-config/
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
# 换行符格式
end_of_line = lf
# 是否在文件最后插入一个空行
insert_final_newline = true
# 是否删除行尾的空格
trim_trailing_whitespace = true
vetur_validation_template = false
# 单/双引号
quote_type = double

[*.md]
trim_trailing_whitespace = false

[*.js]
quote_type = single
